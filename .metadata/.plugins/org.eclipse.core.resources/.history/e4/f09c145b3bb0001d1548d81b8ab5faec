package lv_0;
import java.util.*;

public class EvenOrOdd {

	public static void main(String[] args) {
		int[] num_list = {1, 2, 3, 4, 5};
		
        List<Integer> answer = new ArrayList<>();
        
		int even = 0;
		int odd = 0;
		
		for(int i = 0; i < num_list.length; i += 1) {
			if (num_list[i] % 2 == 0) {
			  even += 1;
			} else {
			  odd += 1;
			}
	
		}
		
	}

}
