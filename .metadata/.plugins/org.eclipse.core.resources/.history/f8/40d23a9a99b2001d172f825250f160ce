package lv_0;

import java.util.Arrays;

public class 배열자르기 {

	public static void main(String[] args) {
		int[] numbers = {1,2,3,4,5};
		int num1 = 1;
		int num2 = 3;

	    int[] answer = new int[3];
	    
	    for(int i = num1; i <= num2; i += 1) {
	    	int x = 0;
	    	answer[0] = numbers[i];
	    	x += 1;
	    }
	    
	    System.out.println(Arrays.toString(answer));
	}

}
